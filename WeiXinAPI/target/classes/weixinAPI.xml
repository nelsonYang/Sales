<?xml version="1.0" encoding="UTF-8"?>
<interfaces>
    <interface>
        <name>getAccessToken</name>
        <url>https://api.weixin.qq.com/cgi-bin/token?grant_type=client_credential</url>
        <method>GET</method>
        <parameters>
            <name>appid</name>
            <name>secret</name>
        </parameters>
        <returnParameters>
            <name>access_token</name>
            <name>expires_in</name>
        </returnParameters>
        <desc>获取token</desc>
    </interface>
    <interface>
        <name>uploadMedia</name>
        <url>http://file.api.weixin.qq.com/cgi-bin/media/upload?</url>
        <method>POST</method>
        <paremeters>
            <name>access_token</name>
            <name>type</name>
            <name>media</name>
        </paremeters>
        <returnParameters>
            <name>type</name>
            <name>media_id</name>
            <name>created_at</name>
        </returnParameters>
        <desc>上传媒体文件</desc>
    </interface>
    <interface>
        <name>downloadMedia</name>
        <url>http://file.api.weixin.qq.com/cgi-bin/media/get?</url>
        <method>GET</method>
        <paremeters>
            <name>access_token</name>
            <name>media_id</name>
        </paremeters>
        <desc>下载媒体文件</desc>
    </interface>
    <interface>
        <name>sendMessage</name>
        <url>https://api.weixin.qq.com/cgi-bin/message/custom/send?</url>
        <method>POST</method>
        <parameters>
            <name>access_token</name>
            <name>touser</name>
            <name>msgtype</name>
            <name>content</name>
            <name>media_id</name>
            <name>title</name>
            <name>description</name>
            <name>musicurl</name>
            <name>hqmusicurl</name>
            <name>thumb_media_id </name>
            <name>url</name>
            <name>picurl</name>
        </parameters>
        <desc>发送客服消息</desc>
    </interface>
    <interface>
        <name>getGroup</name>
        <url>https://api.weixin.qq.com/cgi-bin/groups/get?</url>
        <method>GET</method>
        <parameters>
            <name>access_token</name>
        </parameters>
        <desc>查询分组</desc>
    </interface>
    <interface>
        <name>createGroup</name>
        <url>https://api.weixin.qq.com/cgi-bin/groups/create?</url>
        <method>POST</method>
        <parameters>
            <name>access_token</name>
            <name>name</name>
        </parameters>
        <desc>创建分组</desc>
    </interface>
    <interface>
        <name>updateGroup</name>
        <url>https://api.weixin.qq.com/cgi-bin/groups/update?</url>
        <method>POST</method>
        <parameters>
            <name>access_token</name>
            <name>id</name>
            <name>name</name>
        </parameters>
        <desc>修改分组</desc>
    </interface>
    <interface>
        <name>moveUser</name>
        <url>https://api.weixin.qq.com/cgi-bin/groups/members/update?</url>
        <method>POST</method>
        <parameters>
            <name>access_token</name>
            <name>openid</name>
            <name>to_groupid </name>
        </parameters>
        <desc>移动用户</desc>
    </interface>
    <interface>
        <name>getUserInfo</name>
        <url>https://api.weixin.qq.com/cgi-bin/user/info?</url>
        <method>GET</method>
        <parameters>
            <name>access_token</name>
            <name>openid</name>
        </parameters>
        <desc>获取用户信息</desc>
    </interface>
    <interface>
        <name>getCareUserList</name>
        <url>https://api.weixin.qq.com/cgi-bin/user/get?</url>
        <method>GET</method>
        <parameters>
            <name>access_token</name>
            <name>next_openid</name>
        </parameters>
        <desc>获取关注用户列表</desc>
    </interface>
    <interface>
        <name>createMenu</name>
        <url>https://api.weixin.qq.com/cgi-bin/menu/create?</url>
        <method>POST</method>
        <parameters>
            <name>access_token</name>
            <name>button</name>
            <name>sub_button</name>
            <name>type</name>
            <name>name</name>
            <name>key</name>
            <name>url</name>
        </parameters>
        <desc>创建菜单</desc>
    </interface>
    <interface>
        <name>getMenu</name>
        <url>https://api.weixin.qq.com/cgi-bin/menu/get?</url>
        <method>GET</method>
        <parameters>
            <name>access_token</name>
        </parameters>
        <desc>查询菜单</desc>
    </interface>
    <interface>
        <name>deleteMenu</name>
        <url>https://api.weixin.qq.com/cgi-bin/menu/delete?</url>
        <method>GET</method>
        <parameters>
            <name>access_token</name>
        </parameters>
        <desc>删除菜单</desc>
    </interface>
    <interface>
        <name>getQRCodeName</name>
        <url>https://api.weixin.qq.com/cgi-bin/qrcode/create?</url>
        <method>GET</method>
        <parameters>
            <name>access_token</name>
        </parameters>
         <returnParameters>
            <name>ticket</name>
            <name>expire_seconds</name>
        </returnParameters>
        <desc>获取二维码名字</desc>
    </interface>
    <interface>
        <name>getQRCodeImage</name>
        <url>https://mp.weixin.qq.com/cgi-bin/showqrcode</url>
        <parameters>
            <name>ticket</name>
        </parameters>
        <method>GET</method>
        <desc>获取二维码名字</desc>
    </interface>
    
</interfaces>